										<time class="published" datetime="2017-03-24T18:18">
											19 hours ago
										</time>
									</div>
								</div>

								<div class="more"><svg class="etsk-three-dots-icon"><use xlink:href="icons/icons.svg#etsk-three-dots-icon"></use></svg>
									<ul class="more-dropdown">
										<li>
											<a href="#">Edit Post</a>
										</li>
										<li>
											<a href="#">Delete Post</a>
										</li>
										<li>
											<a href="#">Turn Off Notifications</a>
										</li>
										<li>
											<a href="#">Select as Featured</a>
										</li>
									</ul>
								</div>

							</div>
							<p>{{value.body}}</p>
							<div class="post-additional-info inline-items">

								<a href="#" class="post-add-icon inline-items">
									<svg class="etsk-heart-icon"><use xlink:href="icons/icons.svg#etsk-heart-icon"></use></svg>
									<span>8</span>
								</a>

								<ul class="connections-harmonic">
									<li>
										<a href="#">
											<img src="img/friend-harmonic7.jpg" alt="Connection">
										</a>
									</li>
								</ul>

								<div class="names-people-likes">
									<a href="#">Brooke Anderson</a>, <a href="#">Cameron Thwaits</a> and
									<br>6 more liked this
								</div>


								<div class="comments-shared">
										<a href="#" class="post-add-icon inline-items" name="postComment" id="{{value.id}}">\
										<svg class="etsk-speech-balloon-icon"><use xlink:href="icons/icons.svg#etsk-speech-balloon-icon"></use></svg>
										<span>17</span>
									</a>

									<a href="#" class="post-add-icon inline-items">
										<svg class="etsk-share-icon"><use xlink:href="icons/icons.svg#etsk-share-icon"></use></svg>
										<span>24</span>
									</a>
								</div>

							</div>

							<div class="control-block-button post-control-button">
								{{#if value.likes}}
									<a href="#" class="btn btn-control like" name="postLike" id="{{value.id}}">
								{{else}}
									<a href="#" class="btn btn-control" name="postLike" id="{{value.id}}">
								{{/if}}
									<svg class="etsk-like-post-icon"><use xlink:href="icons/icons.svg#etsk-like-post-icon"></use></svg>
								</a>

								<a href="#" class="btn btn-control" name="postComment" id="{{value.id}}">
									<svg class="etsk-comments-post-icon"><use xlink:href="icons/icons.svg#etsk-comments-post-icon"></use></svg>
								</a>

								<a href="#" class="btn btn-control">
									<svg class="etsk-share-icon"><use xlink:href="icons/icons.svg#etsk-share-icon"></use></svg>
								</a>

							</div>
						</article>
						<ul class="comments-list" name="{{value.id}}">
							{{#each value.comments as |v k|}}
							<li>
								<div class="post__author author vcard inline-items">
									<img src="{{v.avatar}}" alt="author">

									<div class="author-date">
										<a class="h6 post__author-name fn" href="#">{{v.first_name}} {{v.last_name}}</a>
										<div class="post__date">
											<time class="published" datetime="2017-03-24T18:18">
												5 mins ago
											</time>
										</div>
									</div>

									<a href="#" class="more">
										<svg class="etsk-three-dots-icon">
											<use xlink:href="icons/icons.svg#etsk-three-dots-icon"></use>
										</svg>
									</a>
									
								</div>

								<p>{{v.content}}</p>

								<a href="#" class="post-add-icon inline-items">
									<svg class="etsk-heart-icon"><use xlink:href="icons/icons.svg#etsk-heart-icon"></use></svg>
									<span>8</span>
								</a>
								<a href="#" class="reply">Reply</a>
							</li>
							{{/each}}
						</ul>
					</div>					
				{{/each}}
