			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
				<span aria-hidden="true">&times;</span>
			</button>
      	</div>
      	<div class="modal-body">
			<div class="form-group">
				<label for="title">Title</label>
				<input type="text" class="form-control" id="title" name="title" aria-describedby="emailHelp" placeholder="Title">
			</div>
			<div class="form-group">
				<label for="postText"></label>
				<textarea placeholder="Post Content" class="form-control" id="body" name="body" rows="3" wrap="hard" autocomplete="off"></textarea>
			</div>
		</div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id="publish" >Publish</button>
      </div>
    </div>
  </div>
</div>
<script src="/js/jquery-3.2.0.min.js"></script>
<script type="text/javascript">

	$(document).ready(function(){
		
		$('a[data-target="#postModal"]').click(function(e){
			e.preventDefault();
			$('#postModal').modal();
		});

		$('button#publish').click(function(){
			var obj = {
				title: $('input#title').val(),
        		body: $('textarea#body').val()
			};
			$.post('/posts', obj, function(data){
				console.log(data)
				$('#postModal').modal('hide');
			}, 'json');
		});
		
		$(document).find('a[name="postLike"]').click(function(e){
			e.preventDefault();
			var _this = $(this);
			if($(this).hasClass('like')){
				$.ajax({
					url: '/posts/like/'+$(this).attr('id'),
					method: 'PUT',
					data: {},
					success: function(data){
						console.log(data)
						_this.removeClass('like');
					}
				});
			}
			else{
				$.post('/posts/like', {post_id:$(this).attr('id')}, function(data){
					console.log(data);
					_this.addClass('like');
				}, 'json');
			}
		});

		$(document).find('a[name="postComment"]').click(function(e){
			e.preventDefault();
			var id = $(this).attr('id');
			$($(this).parent()).after(
				'<div class="col-md-12 col-sm-12 col-xs-12" id="postCommentContent">'+					
					'<div class="col-md-12">'+
						'<form>'+
							'<input placeholder="Post comment" class="form-control" id="comment" name="comment" required="required"/>'+
							'<button type="button" class="button" id="'+id+'" name="saveComment">publish</button>'+
						'<form>'+
					'</div>'+
				'</div>');
			
			$('button[name="saveComment"]').click(function(e){
				e.preventDefault();
				var obj = {
					post_id:$(this).attr('id'), 
					comment: $('input#comment').val()
				};
				$.post('/posts/comments', obj, function(data){
					
					 $('ul.comments-list[name="'+id+'"]').append(
						'<li>'+
							'<div class="post__author author vcard inline-items">'+
								'<img src="'+data.avatar+'" alt="author">'+
								'<div class="author-date">'+
									'<a class="h6 post__author-name fn" href="#">'+data.first_name+' '+data.last_name+'</a>'+
									'<div class="post__date">'+
										'<time class="published" datetime="2017-03-24T18:18"></time>'+
									'</div>'+
								'</div>'+
								'<a href="#" class="more">'+
									'<svg class="etsk-three-dots-icon">'+
										'<use xlink:href="icons/icons.svg#etsk-three-dots-icon"></use>'+
									'</svg>'+
								'</a>'+
							'</div>'+
							'<p>'+$('input#comment').val()+'</p>'+
							'<a href="#" class="post-add-icon inline-items">'+
								'<svg class="etsk-heart-icon"><use xlink:href="icons/icons.svg#etsk-heart-icon"></use></svg>'+
								'<span>8</span>'+
							'</a>'+
							'<a href="#" class="reply">Reply</a>'+
						'</li>'
					);
					$('div#postCommentContent').remove();
				});
			});
		});

	});
</script>